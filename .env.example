# Discord Bot Configuration
DISCORD_TOKEN=your_discord_bot_token_here

# Discord Channel ID for automatic price updates
# Right-click channel in Discord and "Copy ID" (requires Developer Mode enabled)
DISCORD_CHANNEL_ID=123456789012345678

# Gas Price Configuration
# Area to monitor for gas prices (city, state, zip code, etc.)
GAS_AREA=New York, NY

# Update interval in hours (default: 6)
UPDATE_INTERVAL_HOURS=6

# Optional: API Keys for gas price services (when available)
# GASBUDDY_API_KEY=your_gasbuddy_api_key_here
# AAA_API_KEY=your_aaa_api_key_here